@client.command(pass_context=True)
async def meme(ctx):
    embed = discord.Embed(title="", description="")

    async with aiohttp.ClientSession() as cs:
        async with cs.get('https://www.reddit.com/r/StevenHe/new.json?sort=hot') as r:
            res = await r.json()
            embed.set_image(url=res['data']['children'] [random.randint(0, 25)]['data']['url'])
            await ctx.send(embed=embed)

#I HAVE SET MY REDDIT MEME AS STEVEN HE BUT YOU CAN CHANGE YOURS
#REMEMBER TO IMPORT AIOHTTP WITH "IMPORT AIOHTTP"
